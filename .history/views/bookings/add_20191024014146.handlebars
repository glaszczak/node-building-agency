<div class="card card-body">
  <h3>
    New Booking
  </h3>
  <form action="/bookings/add/newBooking" method="post">
    <div class="form-field mb-2">
      <label for="building">
        Building
      </label>
      <input
        type="text"
        class="form-control"
        name="building"
        value="{{building}}"
        readonly="true"
        required
      />
    </div>
    <div class="form-group mb-2">
      <label for="startDate">
        Start date
      </label>
      <input
        type="date"
        class="form-control"
        name="startDate"
        value={{startDate}}
        readonly="true"
        required
      />
    </div>
    <div class="form-group mb-2">
      <label for="endDate">
        End date
      </label>
      <input
        type="date"
        class="form-control"
        name="endDate"
        value={{endDate}}
        readonly="true"
        required
      />
    </div>
    <div class="form-field mb-2">
      <label for="contractorFilter">
        Available Contractors
      </label>
      <select
        id="contractorFilter"
        class="form-control"
        name="selectContractor"
      >
        <option>
          Choose contractor
        </option>
        {{#each contractors}}
          <option>
            {{this.fullName}}
          </option>
        {{/each}}
      </select>
    </div>
    <button class="btn btn-primary mb-3">
      Submit
    </button>
  </form>
</div>